DEV_PROJECT = "dev"
UAT_PROJECT = "uat"
selectTag = ""
releaseTag = ""
tag = ""

node('dotnet-31') {

  stage('Prepare Variable') {
	script {
      def dateFormat = new SimpleDateFormat("yyyyMMdd")
      def date = new Date()
      def releaseDate = (dateFormat.format(date))
      tag = releaseDate + "-" + currentBuild.number
	  echo "tag: ${tag}"
	}
  }
  

 
}
